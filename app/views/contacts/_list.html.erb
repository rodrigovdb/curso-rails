<table>
  <thead>
    <tr>
      <th>Tipo de Contato</th>
      <th>Número</th>
      <th>Ações</th>
    </tr>
  </thead>

  <tbody>
    <% contacts.each do |contact| %>
      <tr>
        <td><%= contact.contact_type.name %></td>
        <td><%= contact.phone_number %></td>
        <td>
          <%= link_to 'Edit', edit_employee_contact_path(contact.employee, contact) %>
          <%= link_to 'Destroy', employee_contact_path(contact.employee, contact), method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
